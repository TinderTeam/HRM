<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.fuego.misp.domain.view">
	<class name="UserFunctionView" table="view_user_group_function" >
	
		<composite-id>
            <key-property name="userID" column="USER_ID"></key-property>
            <key-property name="functionID" column="FUNCTION_ID" ></key-property>
        </composite-id>        
	</class>
</hibernate-mapping>


